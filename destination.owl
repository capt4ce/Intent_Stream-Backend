    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/capt4ce/ontologies/destination#dest14">
        <rdf:type rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#Beautiful"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#Culture"/>
