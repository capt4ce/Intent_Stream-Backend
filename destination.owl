    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/capt4ce/ontologies/destination#hasNearbyObject">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#hasPlaceObject"/>
