    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/capt4ce/ontologies/destination#hasArea -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/capt4ce/ontologies/destination#hasArea">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#hasPlaceObject"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#hasNearbyTag"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#Area"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/capt4ce/ontologies/destination#hasCity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/capt4ce/ontologies/destination#hasCity">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#hasPlaceObject"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#hasNearbyTag"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/capt4ce/ontologies/destination#City"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


